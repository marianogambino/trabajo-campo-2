package ws;

/**
 * @author Mariano
 *
 */
@WebService(endpointInterface="test.SayHello")
public class TestWs implements IWs{

	private static String HELLO = "Hola ";
	
	/**
	 * 
	 */
	@Override
	public String helloWorld(String name) {
		
		return HELLO+name;
	}

}
